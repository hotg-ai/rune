[package]
name = "{{name}}"
version = "0.1.0"
authors = ["The Rune Developers"]
edition = "2018"
publish = false

[lib]
crate-type = ["cdylib"]
path = "lib.rs"

[dependencies]
{{#each dependencies }}
{{name}} = {{toml deps}}
{{/each}}

[profile.dev]
panic = "abort"

[profile.release]
opt-level = "s"
codegen-units = 1
lto = true

[workspace]
