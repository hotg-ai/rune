[package]
name = "{{name}}"
version = "0.1.0"
authors = ["The Rune Developers"]
edition = "2018"

[lib]
crate-type = ["cdylib"]
path = "lib.rs"

[dependencies]
{{#each dependencies}}
{{#if git}}
{{name}} = { git = "{{git}}" }
{{/if}}
{{#if crates_io}}
{{name}} = "{{crates_io}}"
{{/if}}
{{/each}}

[profile.dev]
panic = "abort"

[profile.release]
opt-level = "s"
codegen-units = 1
lto = true
